{
  "name": "fox-api",
  "displayName": "fox-api",
  "description": "获取Apifox接口",
  "version": "1.0.1",
  "publisher": "lq-fe",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fox-api",
        "title": "Apifox Pull"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "fox-api",
      "properties": {
        "fox-api.apifoxProjectId": {
          "type": "string",
          "default": "",
          "description": "apifox项目id"
        },
        "fox-api.apifoxApiToken": {
          "type": "string",
          "default": "",
          "description": "apifox项目token"
        }
      }
    }
  },
  "scripts": {
    "start": "webpack --mode development --watch",
    "build": "vsce package --allow-missing-repository",
    "vscode:prepublish": "webpack --mode production --devtool hidden-source-map",
    "compile": "tsc -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/prettier": "^3.0.0",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^2.26.1",
    "ts-loader": "^9.5.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "eslint": "^8.57.0",
    "lodash": "^4.17.21",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  }
}
